[Profiles/AAC]
    OutDirectory=~/Music2
    OutPattern=%a - %a%g%A/%d-%D/%n - %t
    Name=AAC
    Format=AAC
    Bitrate=128
    Quality=112
    UseQuality=false
    BitsPerSample=16
    SampleRate=48000
    CreateCue=true
    CueFileName = %a %d - %A.cue
